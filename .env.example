# DashScope API Configuration
DASHSCOPE_API_KEY=YOUR-API-KEY

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=YOUR-PASSWORD

# Processing Configuration
MAX_ASSETS=200  # Set to 0 or leave empty to process all assets, or set to a number like 100 for testing

# 3D Model Base URL (configure this with your actual model hosting URL)
# BASE_URL_TEMPLATE=https://placeholder.com/models/{objaverse_id}.glb
BASE_URL_TEMPLATE=https://huggingface.co/datasets/allenai/objaverse/resolve/main/glbs/{objaverse_dir}/{objaverse_id}.glb?download=true

# Logging
LOG_LEVEL=INFO

